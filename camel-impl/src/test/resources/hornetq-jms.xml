<?xml version="1.0" encoding="UTF-8"?>
<messaging-deployment xmlns="urn:jboss:messaging-deployment:1.0">
  <hornetq-server>
    <jms-destinations>
      <!-- general session -->
      <jms-queue name="JBPM.SESSION">
        <entry name="queue/JBPM.SESSION" />
        <entry name="java:jboss/exported/jms/queue/JBPM.SESSION" />
      </jms-queue>
      <jms-queue name="JBPM.SESSION.RESPONSE">
        <entry name="queue/JBPM.SESSION.RESPONSE" />
        <entry name="java:jboss/exported/jms/queue/JBPM.SESSION.RESPONSE" />
      </jms-queue>
      <!-- general tasks -->
      <jms-queue name="JBPM.TASK">
        <entry name="queue/JBPM.TASK" />
        <entry name="java:jboss/exported/jms/queue/JBPM.TASK" />
      </jms-queue>
      <jms-queue name="JBPM.TASK.RESPONSE">
        <entry name="queue/JBPM.TASK.RESPONSE" />
        <entry name="java:jboss/exported/jms/queue/JBPM.TASK.RESPONSE" />
      </jms-queue>
      <!-- domain test: sessions -->
      <jms-queue name="JBPM.SESSION.DOMAIN.TEST">
        <entry name="queue/JBPM.SESSION.DOMAIN" />
        <entry name="java:jboss/exported/jms/queue/JBPM.SESSION.DOMAIN.TEST" />
      </jms-queue>
      <jms-queue name="JBPM.SESSION.RESPONSE.DOMAIN.TEST">
        <entry name="queue/JBPM.RESPONSE.SESSION.DOMAIN" />
        <entry name="java:jboss/exported/jms/queue/JBPM.SESSION.RESPONSE.DOMAIN.TEST" />
      </jms-queue>
      <!--  domain test: tasks -->
      <jms-queue name="JBPM.TASK.DOMAIN.TEST">
        <entry name="queue/JBPM.TASK.DOMAIN.TEST" />
        <entry name="java:jboss/exported/jms/queue/JBPM.TASK.DOMAIN.TEST" />
      </jms-queue>
      <jms-queue name="JBPM.TASK.RESPONSE.DOMAIN.TEST">
        <entry name="queue/JBPM.TASK.RESPONSE.DOMAIN.TEST" />
        <entry name="java:jboss/exported/jms/queue/JBPM.TASK.RESPONSE.DOMAIN.TEST" />
      </jms-queue>
    </jms-destinations>
  </hornetq-server>
</messaging-deployment>